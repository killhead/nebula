# NebulaGraph Environment Variables
# Copy this file to .env and update the values

# Meta service configuration
NEBULA_META_SERVER_ADDRS=127.0.0.1:9559

# Network configuration
NEBULA_LOCAL_IP=0.0.0.0
NEBULA_PORT=9669
NEBULA_WS_HTTP_PORT=19669

# Authentication (optional)
NEBULA_USER=root
NEBULA_PASSWORD=nebula

# Storage configuration
NEBULA_DATA_PATH=/data
NEBULA_LOG_DIR=/logs

# Performance settings
NEBULA_MAX_HANDLERS_PER_REQ=32
NEBULA_MAX_CONNECTIONS_PER_IP_PER_SECOND=1024
NEBULA_MAX_QUERY_SIZE=4194304

# Memory settings
NEBULA_ROCKSDB_BATCH_SIZE=4096
NEBULA_ROCKSDB_BLOCK_CACHE=67108864

# Logging
NEBULA_LOG_LEVEL=INFO

# Railway specific
RAILWAY_STATIC_URL=
RAILWAY_PUBLIC_DOMAIN=
